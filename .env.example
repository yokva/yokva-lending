VITE_TURNSTILE_SITE_KEY=0x4AAAAA...
# Optional: enable Turnstile in local dev (default: disabled in dev)
# VITE_TURNSTILE_IN_DEV=true

# PostHog
VITE_PUBLIC_POSTHOG_KEY=phc_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
# Direct host (no proxy): https://us.i.posthog.com or https://eu.i.posthog.com
# Reverse proxy on your domain: /ph
VITE_PUBLIC_POSTHOG_HOST=/ph
# Optional UI host (for links/debug)
# VITE_PUBLIC_POSTHOG_UI_HOST=https://us.posthog.com
# Optional: enable PostHog in local dev
# VITE_POSTHOG_IN_DEV=true
# Optional: print PostHog debug logs in browser console
# VITE_POSTHOG_DEBUG=true

# Local/dev proxy target when VITE_PUBLIC_POSTHOG_HOST=/ph
# POSTHOG_PROXY_TARGET=https://us.i.posthog.com

# Cloudflare Pages Function proxy target (production)
# POSTHOG_PROXY_HOST=https://us.i.posthog.com
